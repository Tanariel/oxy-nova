{
    "name": "oxygencms/oxy-nova",
    "description": "Set of tools and boilerplate to use with Laravel Nova.",
    "keywords": ["oxygen", "cms", "laravel", "nova"],
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Blagoev",
            "email": "mb.reppair@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "predis/predis": "^1.1",
        "codezero/laravel-unique-translation": "^1.1",
        "mrmonat/nova-translatable": "^2.0",
        "spatie/laravel-translatable": "^4.1",
        "spatie/laravel-medialibrary": "^7.0.0",
        "ebess/advanced-nova-media-library": "^2.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Oxygencms\\OxyNova\\": "src/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Oxygencms\\OxyNova\\ServiceProvider"
            ]
        }
    }
}
