{
    "name": "oxygencms/oxy-nova",
    "description": "Set of tools and boilerplate to use with Laravel Nova.",
    "keywords": ["oxygen", "cms", "laravel", "nova"],
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Blagoev",
            "email": "mb.reppair@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^7.3.0|^8.0",
        "codezero/laravel-unique-translation": "^3.0",
        "mrmonat/nova-translatable": "^3.0|^4.0",
        "spatie/laravel-translatable": "^4.2|^5.0",
        "spatie/laravel-medialibrary": "^9.0",
        "laravel/nova": "^3.0|^4.0",
        "ebess/advanced-nova-media-library": "^3.0|^4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Oxygencms\\OxyNova\\": "src/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Oxygencms\\OxyNova\\ServiceProvider"
            ]
        }
    }
}
